{
  "name": "omh-web-next",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest test",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "build:start": "next build && next start",
    "tsc-test": "tsc --noEmit -p tsconfig.json",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|tsx|ts|json|yml|yaml|css|md)\"",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "yarn prettier --fix.",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --fix",
    "format": "yarn prettier --write",
    "test-all": "yarn format && yarn check-lint && yarn check-types && yarn build",
    "prepare": "husky install",
    "test:unit": "jest --watch",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "analyze": "cross-env ANALYZE=true yarn build",
    "serve": "serve .next/analyze"
  },
  "dependencies": {
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.9.3",
    "@fontsource/plus-jakarta-sans": "^4.5.8",
    "@mui/icons-material": "^5.8.4",
    "@mui/joy": "^5.0.0-alpha.69",
    "@mui/material": "^5.8.5",
    "@next/bundle-analyzer": "^12.2.2",
    "@supabase/auth-helpers-nextjs": "^0.5.4",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/auth-ui-react": "^0.3.1",
    "@supabase/supabase-js": "^2.8.0",
    "axios": "^0.27.2",
    "formik": "^2.2.9",
    "lodash-es": "^4.17.21",
    "next": "12.1.6",
    "nookies": "^2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.1",
    "react-swipeable-views": "^0.14.0",
    "react-toastify": "^9.1.1",
    "react-use": "^17.4.0",
    "sharp": "^0.30.7",
    "uuidv4": "^6.2.13",
    "yup": "^0.32.11",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.13",
    "@types/react-dom": "18.0.5",
    "@types/react-facebook-login": "^4.1.5",
    "@types/react-swipeable-views": "^0.13.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "concurrently": "^7.2.2",
    "cypress": "^10.1.0",
    "eslint": "^8.0.1",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "^13.0.1",
    "msw": "^0.42.1",
    "prettier": "^2.7.1",
    "standard": "^17.0.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn check-lint",
      "yarn check-types",
      "yarn check-format"
    ]
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
